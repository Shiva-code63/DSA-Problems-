// Majority Element-II
class Solution {
public:
    vector<int> majorityElement(vector<int>& nums) {
        vector<int> v1;
        unordered_map<int,int> m1;
        int n = nums.size();
        int min = (n/3);
        for (int i=0; i<n; i++){
            m1[nums[i]] += 1;
        }
        for (auto &it: m1){
            if (it.second > min){
                v1.push_back(it.first);
            }
        }
        sort(v1.begin(), v1.end());
        return v1;
    }
};
