// Search Insert Position.
class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int n = nums.size();
        int low = 0;
        int high = n-1;
        int result = -1;
        while (low<=high){
            int mid = low + (high-low)/2;
            if (nums[mid] >= target){
                result = mid;
                high = mid-1;
            }
            else{
                low = mid+1;
            }
        }    
        if (result == -1) return n;
        else return result;
    }
};
