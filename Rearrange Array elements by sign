// Rearrange array elements by sign
class Solution {
public:
    vector<int> rearrangeArray(vector<int>& nums) {
        int n = nums.size();
        int pos = 0;
        int neg = 1;
        vector<int> v1(n);
        for (int i=0; i<n; i++){
            if (nums[i]>=0){
                v1[pos] = nums[i];
                pos+=2;
            }
            else{
                v1[neg] = nums[i];
                neg+=2;
            }
        }
        return v1;
    }
};
