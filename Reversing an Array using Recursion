void print8(int i, int arr[], int n ){
    if(i>=n/2) return;                                            // Reversing an array
    swap(arr[i],arr[n-i-1]);
    print8(i+1, arr, n);
}

int main(){
    int n;
    cout<<"Length of array: ";
    cin>>n;
    int arr[n];
    for (int i=0; i<n; i++){
        cin>>arr[i];
    }
    print8(0,arr, n);
    for(int j=0; j<n; j++){
        cout<<arr[j]<<" ";
    }
}
