// Insertion Sort
int main(){
    int n;
    cout<<"Enter the num of elements in the array: ";
    cin>>n;
    int arr[n];
    for (int i=0; i<n; i++){
        cout<<"Enter the element: ";
        cin>>arr[i];
    }
    for (int i=0; i<n; i++){
        int j = i;
        while (j>0 && arr[j]<arr[j-1]){
            swap(arr[j-1], arr[j]);
            j--;
        }
    }
    for (int i=0; i<n; i++){
        cout<<arr[i]<<" ";
    }
}
