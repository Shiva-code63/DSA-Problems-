// Leaders in an Array
class Solution {
public:
    vector<int> leaders(vector<int>& nums) {
        int n = nums.size();
        int max_ele = nums[n-1];
        vector<int> v1;
        v1.push_back(max_ele);
        for (int i = n-2; i>=0; i--){
            if (nums[i] > max_ele){
                max_ele = nums[i];
                v1.push_back(nums[i]);
            }
        }
        reverse(v1.begin(), v1.end());
        return v1;
    }
};
